<fest:template xmlns:fest="http://fest.mail.ru">
    <div id="" class="container">
        <div class="container center" id="header">
            <h2>Register</h2>
        </div>
        <form class="col s12">
            <div class="row">
                <div class="input-field col s12">
                    <input id="email" type="email" class="validate"/>
                    <label for="email">Email</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s12">
                    <input id="password" type="password" class="validate"/>
                    <label for="password">Password</label>
                </div>
            </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="password-repeat" type="password" class="validate"/>
                <label for="password-repeat">Repeat password</label>
            </div>
        </div>
        <div class="row">
            <button class="btn waves-effect waves-light left" type="submit" id="register-input">
                Register
            </button>
            <a class="btn right" id="main">Main menu</a>
        </div>
        </form>
    </div>
</fest:template>
