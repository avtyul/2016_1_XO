<fest:template xmlns:fest="http://fest.mail.ru">
    <div id="register" class="container">
        <div class="container center" id="header">
            <h2>Register</h2>
        </div>
        <form class="col s12" id="register-form">
            <div class="row">
                <div class="input-field col s12">
                    <p>Email</p>
                    <input id="email-input" type="email" class="validate"
                           required="" aria-required="true" placeholder="example@email.com"/>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s12">
                    <p>Password</p>
                    <input id="password-input" type="password" class="validate"
                           required="" aria-required="true"/>

                </div>
            </div>
            <div class="row">
                <button class="btn waves-effect waves-light left" type="submit" id="register-input" href="#">
                    Register
                </button>
                <a class="btn right" id="main" href="#">Main menu</a>
            </div>
        </form>
    </div>
</fest:template>
